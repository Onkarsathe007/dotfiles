{
  "Main Method": {
    "prefix": "main",
    "body": [
      "public static void main(String[] args) {",
      "\t$0",
      "}"
    ],
    "description": "Main method"
  },
  "Public Class": {
    "prefix": "class",
    "body": [
      "public class ${1:ClassName} {",
      "\t$0",
      "}"
    ],
    "description": "Public class"
  },
  "Public Class with Main": {
    "prefix": "classm",
    "body": [
      "public class ${1:ClassName} {",
      "\tpublic static void main(String[] args) {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Public class with main method"
  },
  "Constructor": {
