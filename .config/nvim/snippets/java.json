  "Private Field with Getter/Setter": {
    "prefix": "pgs",
    "body": [
      "private ${1:String} ${2:fieldName};",
      "",
      "public ${1:String} get${2/(.)(.*)/\\u$1$2/}() {",
      "\treturn ${2:fieldName};",
      "}",
      "",
      "public void set${2/(.)(.*)/\\u$1$2/}(${1:String} ${2:fieldName}) {",
      "\tthis.${2:fieldName} = ${2:fieldName};",
      "}"
    ],
    "description": "Private field with getter and setter"
  },
  "System.out.println": {
    "prefix": "sout",
